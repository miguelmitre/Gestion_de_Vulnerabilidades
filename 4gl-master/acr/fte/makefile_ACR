	cp ACRL003.4go  ACRL003.4gi
	@echo "Compilacion de ACRL003 finalizada" #REPORTE DE PROVISION 		
	
ACRB007: ACRB007.4go ACRB0071.frm ACRB0072.frm
	cp ACRB007.4go  ACRB007.4gi
	@echo "Compilacion de ACRB007 finalizada" #LIQUIDA DEVOLUCION DE SALDOS ACREDITADOS 

ACRL004: ACRL004.4go ACRL0041.frm ACRL0042.frm
	cp ACRL004.4go  ACRL004.4gi
	@echo "Compilacion de ACRL004 finalizada" #REPORTE DE LIQUIDACION	

ACRB010: ACRB010.4go ACRB0101.frm
	cp ACRB010.4go  ACRB010.4gi
	@echo "Compilacion de ACRB010 finalizada" #CONSULTA DEVOLUCIONES		
			
ACRB011: ACRB011.4go ACRB0111.frm
	cp ACRB011.4go  ACRB007.4gi
	@echo "Compilacion de ACRB011 finalizada" #REVERSA (CARGA OP.15,GENERACION HISTORICOS,PROVISION,LIQUIDACION)



