.SUFFIXES: .4gl .4go .4gi .per .frm

RUTAFTE = "/safre/acr/fte"     
RUTAEXP = "/safre/acr/exp"

default: 
	clear
	@echo "Compilador del Modulo de Transferencia de Acreditados"
	@echo "====================================================="
	@echo "Modo de Uso:                                         "
	@echo " make PROG                                         "
	@echo " make {TA|TAEXC|43BIS|43BISUSO|43BISEXC|AG|AGEXC}  "
	@echo " make INFONAVIT                                    "
	@echo " make FOVISSSTE                                    "
	@echo                                                     
	@echo " PROG          -> Programa en particular ejemplo: ACRC001 "
	@echo " TA            -> Transferencia de Acreditados            "
	@echo " TAEXC         -> Devolucion de Saldos Excedentes         "
	@echo " 43BIS         -> Solicitud de Marca                      "
	@echo " 43BISUSO      -> Uso de la Garantia                      "
	@echo " 43BISEXC      -> Devolucion de Saldos Excedentes         "
	@echo " AG            -> Anualidades Garantizadas                "
	@echo " AGEXC         -> Devolucion de Saldos Excedentes         "
	@echo " INFONAVIT     -> Modulo Completo de Trasferencia de Acreditados INFONAVIT "
	@echo " FOVISSSTE     -> Transferencia de Acreditados FOVISSSTE  "
	@echo " FOVPEIS       -> Transferencia de Acreditados FOVISSSTE para PENSIONISSSTE "
	@echo " FOV_ICEFAPEIS -> Transferencia de Acreditados FOVISSSTE COMO ICEFA para PENSIONISSSTE "
	@echo


#==============================================================================
# COMPILACION DEL MODULO DE TRANSFERENCIA DE ACREDITADOS INFONAVIT
#==============================================================================

INFONAVIT: TA TAEXC 43BIS 43BISUSO 43BISEXC AG AGEXC



#==============================================================================
# COMPILACION DE TRANSFERENCIA DE ACREDITADOS
#==============================================================================

TA: ACRC001 ACRB001 ACRB002 ACRC002 ACRC004 ACRB008 ACRB009 ACRL001 ACRL002 ACRB030 ACRC003 

ACRC001: ACRC001.4go ACRC0011.frm
	cp ACRC001.4go  ACRC001.4gi
	@echo "Compilacion de ACRC001 finalizada" #RECIBE ARCHIVO DE SOLICITUD DE TRANSFERENCIA (OPERACION 01)

ACRB001: ACRB001.4go ACRB0011.frm
	cp ACRB001.4go  ACRB001.4gi
	@echo "Compilacion de ACRB001 finalizada" #GENERA ARCHIVO DE DEVOLUCION DE SOLICITUDES (OPERACION 06)
	
ACRB002: ACRB002.4go ACRB0021.frm
	cp ACRB002.4go  ACRB002.4gi
	@echo "Compilacion de ACRB002 finalizada" #GENERA ARCHIVO DE SALDOS DE TRANSFERENCIA (OPERACION 09)

ACRL001: ACRL001.4go ACRL0011.frm ACRL0012.frm
	cp ACRL001.4go  ACRL001.4gi
	@echo "Compilacion de ACRL001 finalizada" #REPORTE DE PROVISION
		
ACRC002: ACRC002.4go
	cp ACRC002.4go  ACRC002.4gi
	@echo "Compilacion de ACRC002 finalizada" #RECIBE ARCHIVO DE RECHAZO DE SALDOS DE TRANSFERENCIA	(OPERACION 09)

ACRC004: ACRC004.4go
	cp ACRC004.4go  ACRC004.4gi
	@echo "Compilacion de ACRC004 finalizada" #RECIBE ARCHIVO DE RECHAZO DE DEVOLUCIONES DE SOLICITUDES (OPERACION 06)
	
ACRB008: ACRB008.4go ACRB0081.frm
	cp ACRB008.4go  ACRB008.4gi
	@echo "Compilacion de ACRB008 finalizada" #LIQUIDA SALDOS DE TRANSFERENCIA ACREDITADOS
	
ACRL002: ACRL002.4go ACRL0021.frm ACRL0022.frm
	cp ACRL002.4go  ACRL002.4gi
	@echo "Compilacion de ACRL002 finalizada" #REPORTE DE LIQUIDACION
		
ACRB009: ACRB009.4go ACRB0091.frm
	cp ACRB009.4go  ACRB009.4gi
	@echo "Compilacion de ACRB009 finalizada" #CONSULTA TRANSFERENCIA DE ACREDITADOS			

ACRB030: ACRB030.4go 
	cp ACRB030.4go  ACRB030.4gi
	@echo "Compilacion de ACRB030 finalizada" #RECIBE SOLICITUD DE DESMARCA (OPERACION 30)
	
ACRC003: ACRC003.4go ACRC0031.frm
	cp ACRC003.4go  ACRC003.4gi
	@echo "Compilacion de ACRC003 finalizada" #REVERSA (CARGA OP. 01, GENERACION OP. 06, GENERACION OP. 09, LIQUIDACION)



#==============================================================================
# COMPILACION DE TRANSFERENCIA DE ACREDITADOS-DEVOLUCION DE SALDOS EXCEDENTES
