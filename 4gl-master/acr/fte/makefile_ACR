#==============================================================================
# COMPILACION DEL MODULO DE TRANSFERENCIA DE ACREDITADOS FOVISSSTE
#==============================================================================

FOVISSSTE: ACRC101 ACRC102

ACRC101: ACRC101.4go ACRC1011.frm  
	cp ACRC101.4go  ACRC101.4gi
	@echo "Compilacion de ACRC101 finalizada"  #(CARGO) NOTIFICACION DE SALDOS
	
ACRC102: ACRC102.4go ACRC1021.frm  
	cp ACRC102.4go  ACRC102.4gi
	@echo "Compilacion de ACRC102 finalizada"  #(ABONO) DEVOLUCION DE SALDOS EXCEDENTES


#====================================================================================
# COMPILACION DEL MODULO DE TRANSFERENCIA DE ACREDITADOS FOVISSSTE PARA PENSIONISSSTE
#====================================================================================

FOVPEIS: ACR_GLOB ACRC101PEIS ACRC101_1 ACRR101 ACRC102PEIS ACRC102_1 ACRR102

ACR_GLOB: ACR_GLOB.4go 
	cp ACR_GLOB.4go  ACR_GLOB.4gi
	@echo "Compilacion de ACR_GLOB finalizada"  #FUNCIONES GLOBALES
	
ACRC101PEIS: ACRC101.4go ACRC1011.frm ACRC1012.frm ACRC1013.frm ACRC1014.frm ACRC1015.frm ACRC1016.frm ACRC1017.frm
	cat ACRC101.4go ACR_GLOB.4go > ACRC101.4gi
	@echo "Compilacion de ACRC101 finalizada"  #NOTIFICACION DE SALDOS (CARGO),EJECUTA EN SEGUNDO PLANO EL ACRC101_1
	
ACRC101_1: ACRC101_1.4go
	cat ACRC101_1.4go ACR_GLOB.4go > ACRC101_1.4gi
	@echo "Compilacion de ACRC101_1 finalizada" #CARGA ARCHIVO 

ACRR101: ACRR101.4go ACRR0101.frm
	cat ACRR101.4go ACR_GLOB.4go > ACRR101.4gi
	@echo "Compilacion de ACRR101 finalizada" #REVERSO NOTIFICACION DE SALDOS
		
ACRC102PEIS: ACRC102.4go ACRC1021.frm  ACRC1022.frm ACRC1023.frm ACRC1024.frm
	cat ACRC102.4go ACR_GLOB.4go > ACRC102.4gi
	@echo "Compilacion de ACRC102 finalizada"  #DEVOLUCION DE SALDOS EXCEDENTES (ABONO),EJECUTA EN SEGUNDO PLANO EL ACRC102_1	

ACRC102_1: ACRC102_1.4go
	cat ACRC102_1.4go ACR_GLOB.4go > ACRC102_1.4gi
	@echo "Compilacion de ACRC102_1 finalizada" 	#CARGA ARCHIVO 

ACRR102: ACRR102.4go ACRR0101.frm
	cat ACRR102.4go ACR_GLOB.4go > ACRR102.4gi
	@echo "Compilacion de ACRR102 finalizada" #REVERSO DEVOLUCION DE SALDOS EXCEDENTES


#===============================================================================================
# COMPILACION DEL MODULO DE TRANSFERENCIA DE ACREDITADOS FOVISSSTE COMO ICEFA PARA PENSIONISSSTE
#===============================================================================================

FOV_ICEFAPEIS: ACRC103 ACRC103_1 ACRR103 ACRC104 ACRC104_1 ACRR104

ACRC103: ACR_GLOB ACRC103_1 ACRC103.4go ACRC1016.frm ACRC1017.frm ACRC1031.frm ACRC1032.frm ACRC1033.frm ACRC1034.frm ACRC1035.frm ACRC1036.frm
	cat ACRC103.4go ACR_GLOB.4go > ACRC103.4gi
	@echo "Compilacion de ACRC103 finalizada"  #(CARGO) NOTIFICACION DE SALDOS,SOLO PARA PENSIONISSSTE SE EJECUTA EN SEGUNDO PLANO EL ACRC103_1
	
ACRC103_1: ACR_GLOB ACRC103_1.4go
	cat ACRC103_1.4go ACR_GLOB.4go > ACRC103_1.4gi
	@echo "Compilacion de ACRC103_1 finalizada" 

ACRR103: ACRR103.4go ACRR0101.frm
	cat ACRR103.4go ACR_GLOB.4go > ACRR103.4gi
	@echo "Compilacion de ACRR103 finalizada" #REVERSO NOTIFICACION DE SALDOS
	
ACRC104: ACR_GLOB ACRC104_1 ACRC104.4go ACRC1041.frm ACRC1042.frm ACRC1043.frm ACRC1044.frm ACRC1016.frm ACRC1017.frm
	cat ACRC104.4go ACR_GLOB.4go > ACRC104.4gi
	@echo "Compilacion de ACRC104 finalizada" #(ABONO) DEVOLUCION DE SALDOS EXCEDENTES,SOLO PARA PENSIONISSSTE SE EJECUTA EN SEGUNDO PLANO EL ACRC104_1	

ACRC104_1: ACR_GLOB ACRC104_1.4go
	cat ACRC104_1.4go ACR_GLOB.4go > ACRC104_1.4gi
	@echo "Compilacion de ACRC104_1 finalizada" 

ACRR104: ACRR104.4go ACRR0101.frm
	cat ACRR104.4go ACR_GLOB.4go > ACRR104.4gi
	@echo "Compilacion de ACRR104 finalizada" #REVERSO DEVOLUCION DE SALDOS EXCEDENTES
		
	
	
#==============================================================================	
# D I R E C T I V A S   D E   C O M P I L A C I O N   E S P E C I A L E S
#==============================================================================


libera4gi:
	cp $(EJEC).4gi $(RUTAEXP)/$(EJEC).4gi

liberafrm:	
	cp $(FORM).frm $(RUTAEXP)/$(FORM).frm

depura:
	rm -f *.4go *.4gi *.frm *.err

#===============================================================================
# R E G L A S  D E  C O M P I L A C I O N           
#===============================================================================
	
.4gl.4go:
	fglpc $*
.per.frm:
	form4gl $*


