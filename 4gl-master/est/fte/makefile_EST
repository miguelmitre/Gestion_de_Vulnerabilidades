#####################################################################
#Descripcion       => COMPILACION DEL MODULO DE ESTADISTICAS        #
#Sistema           => EST                                           #
#Fecha creacion    => 30 OCUBRE 2012                                #
#Por               => STEFANIE DANIELA VERA PIÑA                    #
#####################################################################

.SUFFIXES: .4gl .4go .4gi .per .frm

RUTAFTE = "/safre/est/fte"     
RUTAEXP = "/safre/est/exp"

compila: ESTM011 ESTB102 ESTB102_1


#==============================================================================
# COMPILACION DEL MODULO DE ESTADISTICAS DE VOLUNTARIAS
#==============================================================================


ESTM011: ESTM011.4go ESTM0111.frm
	cp ESTM011.4go  ESTM011.4gi
	@echo "Compilacion de ESTM011 finalizada" #GENERA ESTADISTICA 0502 DE VOLUNTARIAS 
	
ESTB102: ESTB102.4go ESTB1021.frm
	cp ESTB102.4go  ESTB102.4gi
	@echo "Compilacion de ESTB102 finalizada"	#GENERA ANEXO "C" DE VOLUNTARIAS, EJECUTA EN SEGUNDO PLANO EL EXCB102_1 PARA PENSIONISSSTE

#==============================================================================
# COMPILACION DE LA EJECUCION POR NOHUP DEL ANEXO "C" PENSIONISSSTE
#==============================================================================

ESTB102_1: ESTB102_1.4go 
	cp ESTB102_1.4go  ESTB102_1.4gi
	@echo "Compilacion de ESTB102_1 finalizada"	#GENERA EN SEGUNDO PLANO ANEXO "C" DE VOLUNTARIAS	
	
	
#==============================================================================	
# D I R E C T I V A S   D E   C O M P I L A C I O N   E S P E C I A L E S
#==============================================================================


libera4gi:
	cp $(EJEC).4gi $(RUTAEXP)/$(EJEC).4gi

liberafrm:	
	cp $(FORM).frm $(RUTAEXP)/$(FORM).frm

depura:
	rm -f *.4go *.4gi *.frm *.err

#===============================================================================
# R E G L A S  D E  C O M P I L A C I O N           
#===============================================================================
	
.4gl.4go:
	fglpc $*
.per.frm:
	form4gl $*


