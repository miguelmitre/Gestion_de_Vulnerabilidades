DATABASE safre_af
SCREEN
{
\g----------------------------------------------------------------------------\g


 	        Codigo Afore  : [aa        ]
                Razon social  : [b                                       ]
                Representante : [c                                       ]
                Calle         : [d                                       ]
                Numero        : [e         ]
                Depto         : [f         ]
                Cod.Postal    : [k         ]
                Colonia       : [g                           ]
                Delegacion    : [h     ][h1                             ]
                Ciudad        : [i     ][i1                             ]
                Estado        : [j     ][j1                             ]
