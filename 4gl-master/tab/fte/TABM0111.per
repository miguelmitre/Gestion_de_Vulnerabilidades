}
TABLES
tab_afore_local
ATTRIBUTES
aa=tab_afore_local.codigo_afore;
b=tab_afore_local.razon_social     ,UPSHIFT;
c=tab_afore_local.representante    ,UPSHIFT;
d=tab_afore_local.calle            ,UPSHIFT;
e=tab_afore_local.numero           ,UPSHIFT;
f=tab_afore_local.depto            ,UPSHIFT;
k=tab_afore_local.cod_postal       ,UPSHIFT;
g=tab_afore_local.colonia          ,UPSHIFT,NOENTRY;
h=tab_afore_local.delegacion       ,UPSHIFT,NOENTRY;
