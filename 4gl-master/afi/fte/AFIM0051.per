DATABASE safre_af
SCREEN
{

N.S.S.    [aa         ]  RFC [cc           ]   C.U.R.P.  [bb                ]
Paterno   [ee                        ]  Materno  [ff                        ]
Nombres   [gg                        ]  Fecha de Nacimiento      [jj        ]
No. Folio [fff       ]   Tipo solicitud   [rr]  [ww]
-----------------------------------------------------------------------------
Registro Patronal [a          ] RFC Patronal      [c            ]     
Razon Social      [b                                          ]
Telefono          [d              ]
Calle                   [e                                                 ] 
Numero                  [f                   ] 
CP                      [g    ]  
Colonia                 [h                                                 ]
Delegacion / Municipio  [i    ] [ii                                        ]

Ciudad                  [j    ] [k                                         ]
Estado                  [l    ] [m                                         ]

}
