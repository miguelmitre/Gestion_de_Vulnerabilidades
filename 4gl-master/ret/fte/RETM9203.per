c01 = FORMONLY.consecutivo                  TYPE CHAR,UPSHIFT,NOENTRY                ; 
c02 = FORMONLY.usuario_captura              TYPE CHAR,UPSHIFT,NOENTRY                ;
c04 = FORMONLY.fecha_solicitud              TYPE DATE,        FORMAT = "dd/mm/yyyy"  ;
c03 = FORMONLY.fecha_captura                TYPE DATE,        FORMAT = "dd/mm/yyyy"  ;
c05 = FORMONLY.folio                        TYPE CHAR,UPSHIFT                        ;

d   = FORMONLY.constancia                   TYPE CHAR,UPSHIFT,INCLUDE = ("S","N")    ;
