ATTRIBUTES
a=FORMONLY.n_seguro        TYPE CHAR;
b=FORMONLY.nombre          TYPE CHAR,NOENTRY;
c=FORMONLY.fec_1           TYPE DATE,FORMAT="dd-mm-yyyy",NOENTRY;
d=FORMONLY.fec_2           TYPE DATE,FORMAT="dd-mm-yyyy",NOENTRY;

mm=FORMONLY.marca          TYPE CHAR;
fm=FORMONLY.fec_marca      TYPE DATE,FORMAT="dd-mm-yyyy",NOENTRY;
am=FORMONLY.actividad      TYPE CHAR;

e=FORMONLY.subct_cod       TYPE SMALLINT, NOENTRY;
f=FORMONLY.subct_corta     TYPE CHAR, NOENTRY;
g=FORMONLY.monto_pesos     TYPE DECIMAL, FORMAT= "-------&.&&", NOENTRY;
h=FORMONLY.monto_acciones  TYPE DECIMAL, FORMAT= "-------&.&&", NOENTRY;

i=FORMONLY.tmonto_pesos    TYPE DECIMAL, FORMAT= "--------&.&&", NOENTRY;
j=FORMONLY.tmonto_acciones TYPE DECIMAL, FORMAT= "-------&.&&", NOENTRY;
k=FORMONLY.tmonto_vivienda TYPE DECIMAL, FORMAT= "--------&.&&", NOENTRY;
l=FORMONLY.tmonto_cero     TYPE DECIMAL, FORMAT= "-------&.&&", NOENTRY;
aa=FORMONLY.tcuenta_pesos  TYPE DECIMAL, FORMAT ="--------&.&&", NOENTRY;
bb=FORMONLY.tcuenta_accion TYPE DECIMAL, FORMAT ="-------&.&&", NOENTRY;

m=FORMONLY.desc_larga      TYPE CHAR, UPSHIFT, NOENTRY;

n=FORMONLY.f_pago          TYPE DATE,FORMAT="dd-mm-yy",NOENTRY;
