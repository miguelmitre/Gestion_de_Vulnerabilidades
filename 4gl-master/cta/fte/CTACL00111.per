x=FORMONLY.folio_sua       TYPE CHAR, INVISIBLE, NOENTRY;
y=FORMONLY.folio           TYPE CHAR, INVISIBLE, NOENTRY;
z=FORMONLY.consecutivo    TYPE INTEGER, INVISIBLE, NOENTRY;

s=FORMONLY.ttmonto_pesos    TYPE DECIMAL, FORMAT = "------&.&&", NOENTRY;
t=FORMONLY.ttmonto_acciones TYPE DECIMAL, FORMAT = "-------&.&&", NOENTRY;

u=FORMONLY.vfecha_accion    TYPE DATE, FORMAT="dd-mm", NOENTRY;
w=FORMONLY.vprecio_accion   TYPE DECIMAL, FORMAT = "##&.&&&&&&", NOENTRY;

INSTRUCTIONS
SCREEN RECORD scr_1[10](FORMONLY.subct_cod THRU FORMONLY.monto_acciones);
SCREEN RECORD scr_2[8](FORMONLY.f_pago THRU FORMONLY.consecutivo);
