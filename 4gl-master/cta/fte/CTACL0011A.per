ATTRIBUTES
a=FORMONLY.n_seguro        TYPE CHAR;
b=FORMONLY.nombre          TYPE CHAR,NOENTRY;

mm=FORMONLY.marca          TYPE CHAR;
fm=FORMONLY.fec_marca      TYPE DATE,FORMAT="dd-mm-yyyy",NOENTRY;
f1=FORMONLY.fec_1          TYPE DATE,INVISIBLE,NOENTRY;
f2=FORMONLY.fec_2          TYPE DATE,INVISIBLE,NOENTRY;
am=FORMONLY.actividad      TYPE CHAR;

l=FORMONLY.subct_prueba      TYPE CHAR;
e=FORMONLY.subct_cod         TYPE SMALLINT, NOENTRY;
f=FORMONLY.subct_corta       TYPE CHAR, NOENTRY;
g=FORMONLY.monto_pesos       TYPE DECIMAL, FORMAT= "-------&.&&&&&&", NOENTRY;
h=FORMONLY.monto_acciones    TYPE DECIMAL, INVISIBLE, NOENTRY;

s=FORMONLY.siefore_cod       TYPE SMALLINT, NOENTRY;
w=FORMONLY.siefore_nom       TYPE CHAR, NOENTRY;
s1=FORMONLY.pesos_sie        TYPE DECIMAL, FORMAT= "-------&.&&&&&&", NOENTRY;
s2=FORMONLY.acciones_sie     TYPE DECIMAL, FORMAT= "-------&.&&&&&&", NOENTRY;


--m=FORMONLY.desc_larga      TYPE CHAR,WORDWRAP, NOENTRY;
