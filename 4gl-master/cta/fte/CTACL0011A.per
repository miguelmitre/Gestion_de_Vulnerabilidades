y=FORMONLY.folio           TYPE CHAR, INVISIBLE, NOENTRY;
z=FORMONLY.consecutivo     TYPE INTEGER, INVISIBLE, NOENTRY;



INSTRUCTIONS
SCREEN RECORD scr_1[8](FORMONLY.subct_prueba THRU FORMONLY.monto_acciones);
SCREEN RECORD scr_2[5](FORMONLY.f_pago THRU FORMONLY.consecutivo);
SCREEN RECORD scr_3[7](FORMONLY.siefore_cod THRU FORMONLY.acciones_sie);
